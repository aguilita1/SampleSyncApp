{
  "name": "aguilita1/samplesyncapp",
  "description": "Sample Sync Application written in PHP to test out Github Actions.",
  "keywords": [
    "Sample PHP App",
    "React EventLoop",
    "Github Actions"
  ],
  "homepage": "https://github.com/aguilita1/SampleSyncApp",
  "license": "AGPL-3.0-only",
  "authors": [
    {
      "name": "Daniel I. Kelley",
      "homepage": "https://github.com/aguilita1"
    }
  ],
  "config": {
      "platform": {
           "php": "8.3.10"
       }
  },
  "require": {
    "php": ">=8.2",
    "guzzlehttp/guzzle": "^7.9.2",
    "react/event-loop":"^1.5.0",
    "monolog/monolog":"^3.7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.3.1",
    "zircote/swagger-php": "~4.10.6",
      "phpstan/phpstan": "^1.11.11"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "stable"
}
